net: "train_confidence.prototxt"

test_interval: 1
test_iter: 1
display: 1

# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.1
lr_policy: "inv"
gamma: 0.0001
momentum: 0.3
power: 0.75
weight_decay: 0.0005


# The maximum number of iterations
max_iter: 100

# snapshot intermediate results
snapshot_prefix: "nusc_baidu_confidence"

solver_mode: GPU
