net: "data/train_confidence.prototxt"

test_interval: 1
test_iter: 100
display: 1

# The base learning rate, momentum and the weight decay of the network.


# base_lr: 0.0000001
base_lr: 0.00001
lr_policy: "fixed"

# lr_policy: "inv"
# gamma: 0.0001
# momentum: 0.9
# power: 0.75
# weight_decay: 0.0005


# The maximum number of iterations
max_iter: 10000

# snapshot intermediate results
snapshot_prefix: "nusc_baidu_confidence"

solver_mode: GPU
